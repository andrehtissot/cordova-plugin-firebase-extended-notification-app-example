<?xml version='1.0' encoding='utf-8'?>
<widget id="com.andretissot.firebaseextendednotification.exampleapp" version="1.3.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Firebase Extended Notification App Example</name>
    <description>
        A sample Apache Cordova application that makes use of cordova-plugin-firebase-extended-notification
    </description>
    <author email="andre_a_tissot@yahoo.com.br">Andr√© Augusto Tissot</author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
    <plugin name="cordova-plugin-firebase-extended-notification" spec="^2.0.0" />
</widget>
